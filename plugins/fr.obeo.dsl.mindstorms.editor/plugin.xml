<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated mindstorms -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.obeo.dsl.mindstorms.presentation.MindstormsModelWizardID"
            name="%_UI_MindstormsModelWizard_label"
            class="fr.obeo.dsl.mindstorms.presentation.MindstormsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MindstormsModelFile.gif">
         <description>%_UI_MindstormsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated mindstorms -->
      <editor
            id="fr.obeo.dsl.mindstorms.presentation.MindstormsEditorID"
            name="%_UI_MindstormsEditor_label"
            icon="icons/full/obj16/MindstormsModelFile.gif"
            extensions="mindstorms"
            class="fr.obeo.dsl.mindstorms.presentation.MindstormsEditor"
            contributorClass="fr.obeo.dsl.mindstorms.presentation.MindstormsActionBarContributor">
      </editor>
   </extension>

</plugin>
